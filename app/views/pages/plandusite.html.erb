<% content_for :meta_title, "Voici tous les liens du site alexandrecordani.com - agence immobilière saint maur des fossés" %>
<% content_for :meta_description, "Vous trouverez sur cette page tous les liens du site alexandrecordani.com " %>

<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h4>Les rues de Saint-Maur-Des-Fossés</h4>
			<% @prices.each do |price| %>
				<p><%= link_to price.address, price_path(price) %></p>
			<% end %>
		</div>
		<div class="col-md-6">
			<h4>Les parcelles de St Maur</h4>
			<% @estimations.each do |estimation| %>
				<p><%= link_to estimation.full_street_address, estimation_path(estimation) %></p>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-6">
			<h4>Les annonces de biens à vendre</h4>
			<% @sales.each do |sale| %>
				<p><%= link_to sale.title, sale_path(sale) %></p>
			<% end %>
		</div>
	</div>
	
</div>