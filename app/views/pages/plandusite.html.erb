<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h4>Les rues de Saint-Maur-Des-Fossés</h4>
			<% @prices.each do |price| %>
				<p><%= link_to price.address, price_path(price) %></p>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-12">
			<h4>Les annonces de biens à vendre</h4>
			<% @sales.each do |sale| %>
				<p><%= link_to sale.title, sale_path(sale) %></p>
			<% end %>
		</div>
	</div>
	
</div>